function doGet() {
  var htmlOutput = HtmlService.createHtmlOutputFromFile('chatbot.html');
  return htmlOutput.setXFrameOptionsMode(HtmlService.XFrameOptionsMode.ALLOWALL);
}

function retrieveAdditionalContentFromDoc() {
  var documentId = 'YOUR_DOC_ID'; // Replace with the ID of your Google Docs file
  var content = '';

  var doc = DocumentApp.openById(documentId);
  var paragraphs = doc.getBody().getParagraphs();

  for (var i = 0; i < paragraphs.length; i++) {
    content += paragraphs[i].getText();
  }

  return content;
}
